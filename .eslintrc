{
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": 0,
        "no-undef": 0,
        "no-unused-vars": 0,
        "max-len": [1, 140, 2, { "ignoreComments": true }],
        "react/prop-types": [
            2
        ]
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ]
}